---
title: PIC..ME Gallery
layout: 'base.njk'
---
    <!-- navbar -->
    <div class="navbar-fixed">
        <nav class="white">
            <div class="container">
                <div class="nav-wrapper">
                    <h4 class="left-align black-text">
                        My<span> Gallery</span>
                    </h4>
                </div>
            </div>
        </nav>
    </div>
        <div class="container">
            <div class="fixed-action-btn">
                <a href="/" class="btn-floating btn-large black"> <i class="material-icons "> home </i> home</a>
            </div>
        <div class="pics section z-depth-2">
            <div class="row">
                {% for galleryPhoto in collections.galleryPhoto | reverse %}
                   {% set length = galleryPhoto.data.links.length %} 
                     {% for i in range(0, length) %}
                     <div class="col s12 m4">
                        <img src="{{ galleryPhoto.data.links[i] }}" alt="" class="materialboxed responsive-img">
                    </div>
                   {% endfor %}
                {% endfor %}
                
            </div>
        </div>

    </div>  
</body>
<script>
    //material boxed
    const mb = document.querySelectorAll('.materialboxed');
        M.Materialbox.init(mb, {});
        //scrollspy
        const ss = document.querySelectorAll('.scrollspy');
        M.ScrollSpy.init(ss, {});
</script>